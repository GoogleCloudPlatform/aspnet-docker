steps:
- name: '${STAGING_BUILDER_IMAGE}'
  args:
    - '--version-map'
    - '1.0.7=gcr.io/aspnetcore-staging/aspnetcore:1.0.7'
    - '1.1.4=gcr.io/aspnetcore-staging/aspnetcore:1.1.4'
    - '2.0.0=gcr.io/aspnetcore-staging/aspnetcore:2.0.0'
- name: gcr.io/cloud-builders/docker:latest
  args: [ 'build', '-t', '$_OUTPUT_IMAGE', '--no-cache', '--pull', '.' ]
images:
  ['$_OUTPUT_IMAGE']
